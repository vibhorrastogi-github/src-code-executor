	<language name="c">
		<compileCmd>c++ -o #FILE_NAME </compileCmd>
		<compileArg></compileArg>
		<executeCmd></executeCmd>
		<executeArg>.EXE</executeArg>
		<createFolder>false</createFolder>
		<generatedFileExtensions>.c,.EXE,.OBJ</generatedFileExtensions>
	</language>
	<language name="cpp">
		<compileCmd>c++ -o #FILE_NAME </compileCmd>
		<compileArg></compileArg>
		<executeCmd></executeCmd>
		<executeArg>.EXE</executeArg>
		<createFolder>false</createFolder>
		<generatedFileExtensions>.cpp,.EXE,.OBJ</generatedFileExtensions>
	</language>
	<language name="java">
		<compileCmd>javac #FOLDER_NAME/</compileCmd>
		<compileArg></compileArg>
		<executeCmd>java -cp #FOLDER_NAME/ </executeCmd>
		<executeArg> -Xmx5m</executeArg>
		<createFolder>true</createFolder>
		<generatedFileExtensions>.java,.class</generatedFileExtensions>
	</language>
</commandBox>